{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Ensemble Engine",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/apps/ensemble-engine/main.js",
      "outFiles": ["${workspaceFolder}/dist/apps/ensemble-engine/**/*.js"],
      "cwd": "${workspaceFolder}/apps/ensemble-engine",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
